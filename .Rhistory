#' Title
#'
#' @param mat
#'
#' @return
#' @export
#'
#' @examples
as.sparse.matrix <- function(mat) {
mat <- data.table::as.data.table(mat)
colnames(mat) <- paste0(1:ncol(mat))
cat(is.data.table(mat))
mat <- mat[, row := 1:.N]
mat <- data.table::melt(mat, id.vars = "row", na.rm = TRUE,
variable.name = "col") %>%
.[, col := col %>% as.integer] %>%
.[]
return(mat)
}
